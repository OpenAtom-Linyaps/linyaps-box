{
	"name": "Check rlimits with `linyaps-box-st:stable-slim`",
	"image": "docker.io/comixhe1895/linyaps-box-st:stable-slim",
	"process": {
		"user": {
			"uid": 0,
			"gid": 0
		},
		"args": ["/bin/env", "bash", "-c", "prlimit --nofile --noheadings -p $$ | awk '{print $7,$8}'"],
		"env": [
			"PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
			"TERM=xterm",
			"HOME=/root"
		],
		"cwd": "/",
		"rlimits": [
			{
				"type": "RLIMIT_NOFILE",
				"hard": 1024,
				"soft": 1024
			}
		]
	},
	"expected": "1024 1024"
}
